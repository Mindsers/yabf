{
  "name": "yabf",
  "version": "2.0.2",
  "description": "Yet Another Basic Framework for NodeJS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:Mindsers/yabf.git",
  "author": "Mindsers <dev@nathanaelcherrier.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "posttest": "rm -rf tests",
    "test:unit": "ava tests/**/*.spec.js",
    "test:lint": "tslint -p tsconfig.json -c tslint.json 'src/**/*.ts'",
    "test": "yarn test:lint && yarn test:unit",
    "pretest": "yarn build:test",
    "build:test": "tsc -p tsconfig.test.json",
    "build": "tsc",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@types/node": "10.14.4",
    "ava": "0.25.0",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.0.3"
  },
  "engines": {
    "yarn": "^1.0.0"
  }
}
